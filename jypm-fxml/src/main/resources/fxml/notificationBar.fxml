<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane fx:id="rootPane" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
          minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="800.0"
          stylesheets="@../stylesheet.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.github.open96.jypm.fxml.NotificationBarController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="110.0" minWidth="-Infinity" prefWidth="110.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="110.0" minWidth="-Infinity" prefWidth="110.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="110.0" minWidth="-Infinity" prefWidth="110.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefWidth="331.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="110.0" minWidth="-Infinity" prefWidth="110.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Button fx:id="syncButton" alignment="CENTER" contentDisplay="CENTER" minHeight="26.0" minWidth="100.0"
                mnemonicParsing="false" onAction="#onSyncButtonClick" text="Sync all" textAlignment="CENTER"
                GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
        <Button fx:id="settingsButton" alignment="CENTER" contentDisplay="CENTER" minHeight="26.0" minWidth="100.0"
                mnemonicParsing="false" onAction="#onSettingsButtonClick" text="Settings" textAlignment="CENTER"
                GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
        <Button fx:id="addPlaylistButton" alignment="CENTER" contentDisplay="CENTER" minHeight="26.0" minWidth="100.0"
                mnemonicParsing="false" onAction="#onAddPlaylistButtonClick" text="Add playlist" textAlignment="CENTER"
                GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
        <Label fx:id="notificationText" alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308"
               maxWidth="1.7976931348623157E308" prefWidth="355.0" text="..." textAlignment="CENTER"
               GridPane.columnIndex="3"/>
        <Button fx:id="detailsButton" alignment="CENTER" contentDisplay="CENTER" minHeight="26.0" minWidth="100.0"
                mnemonicParsing="false" onAction="#onDetailsButtonClick" text="Details" textAlignment="CENTER"
                GridPane.columnIndex="4" GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
    </children>
</GridPane>
